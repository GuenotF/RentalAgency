<form method="post" action="{{ path('agence_delete', {'idagence': agence.idagence}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ agence.idagence) }}">
    <button class="btn">Delete</button>
</form>
